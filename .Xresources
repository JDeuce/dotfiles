dzen2.font: Inconsolata:size=9

URxvt*saveLines:         0
URxvt.urgentOnBell:         true
URxvt.buffered:             false
URxvt.scrollBar_right:      false
URxvt.scrollBar:            false
URxvt.borderLess:           false
!URxvt.perl-ext:             matcher,selection-to-clipboard,confirm-paste,keyboard-select
URxvt.url-launcher:         firefox
URxvt.matcher.button:       1
!URxvt.keysym.M-u:           matcher:list
!URxvt.keysym.M-Escape:      perl:keyboard-select:activate
!URxvt.keysym.M-s:           perl:keyboard-select:search
!URxvt.font:                 xft:inconsolata:antialias=true:size=8
URxvt.font:                 xft:Inconsolata\\-dz for Powerline:size=8


! to match gnome-terminal "Linux console" scheme
! foreground/background
URxvt*background: #121212
URxvt*foreground: #ffffff
! black
URxvt.color0  : #000000
URxvt.color8  : #4C4C4C
! red
URxvt.color1  : #CD0000
URxvt.color9  : #FF0000
! green
URxvt.color2  : #00CD00
URxvt.color10 : #00FF00
! yellow
URxvt.color3  : #CDCD00
URxvt.color11 : #FFFF00
! blue
URxvt.color4  : #1E90FF
URxvt.color12 : #4682B4
! magenta
URxvt.color5  : #CD00CD
URxvt.color13 : #FF00FF
! cyan
URxvt.color6  : #00CDCD
URxvt.color14 : #00FFFF
! white
URxvt.color7  : #E5E5E5
URxvt.color15 : #FFFFFF
